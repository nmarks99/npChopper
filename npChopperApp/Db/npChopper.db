record(longout, "$(P)HarmonicMultSet") {
    field(DTYP, "asynInt32")
    field(DRVH, 15)
    field(DRVL, 1)
    field(OUT,  "@asyn($(PORT),$(ADDR=0),$(TIMEOUT=1))HARMONIC_MULT")
}