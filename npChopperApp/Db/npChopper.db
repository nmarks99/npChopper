record(longin, "$(P)FreqSync") {
    field(DTYP, "asynInt32")
    field(INP,  "@asyn($(PORT),$(ADDR=0),$(TIMEOUT=1))FREQ_SYNC_MEASURE")
    field(SCAN, "I/O Intr")
}

