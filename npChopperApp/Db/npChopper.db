record(ai, "$(P)FreqSync") {
    field(DTYP, "asynFloat64")
    field(INP,  "@asyn($(PORT),$(ADDR=0),$(TIMEOUT=1))FREQ_SYNC")
    field(SCAN, "I/O Intr")
}

