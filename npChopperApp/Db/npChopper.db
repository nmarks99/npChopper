record(ai, "$(P)FreqSyncRBV")
{
    field(DTYP, "asynFloat64")
    field(INP,  "@asyn($(PORT),$(ADDR=0),$(TIMEOUT=1))FREQ_SYNC_IN")
    field(SCAN, "I/O Intr")
}

